{
  "name": "lasso-postcss",
  "version": "0.4.0",
  "description": "Lasso plugin to transform CSS using PostCSS.",
  "keywords": [
    "lasso-plugin",
    "lasso",
    "marko",
    "lasso-marko",
    "postcss",
    "postcss-runner",
    "css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WeAreGenki/lasso-postcss.git"
  },
  "author": "Max Milton <max@wearegenki.com>",
  "license": "Apache-2.0",
  "homepage": "https://wearegenki.com/pro#OpenSource",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js src test",
    "pretest": "npm run lint",
    "test": "jest --config test/jest.config.js --coverage",
    "test-ci": "yarn test --runInBand",
    "test:watch": "yarn test --notify --watch"
  },
  "dependencies": {
    "postcss": "^6.0.17",
    "postcss-load-config": "^1.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@wearegenki/eslint-config": "0.5.0-alpha.f1eec7f5",
    "@wearegenki/test": "0.5.0-alpha.f1eec7f5",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "eslint": "4.18.2",
    "eslint-plugin-jest": "21.13.0",
    "jest": "22.4.2",
    "lasso": "3.1.2"
  },
  "engines": {
    "node": ">= 6.4.0",
    "npm": ">= 3.10.0"
  }
}
